<?xml version='1.0' encoding="UTF-8" standalone="yes" ?>
<!DOCTYPE WMT_MS_Capabilities SYSTEM
 "http://www.digitalearth.gov/wmt/xml/capabilities_1_0_0.dtd">
<WMT_MS_Capabilities version="1.0.0">
  <!-- Service Metadata -->
  <Service>
    <!-- The WMT-defined name for this type of service -->
    <Name>GetMap</Name>
    <!-- Human-readable title for pick lists -->
    <Title>Iowa Geographic Image Map Server</Title>
    <!-- Narrative description providing additional information -->
    <Abstract>WMT Map Server maintained by USDA-NRCS and Iowa State University.
    Contact: gahadish@iastate.edu</Abstract>
    <Keywords>DOQ</Keywords>
    <!-- Top-level address of service or service provider.  See also OnlineResource attributes of <dcpType> children. -->
    <OnlineResource>http://belize.gis.iastate.edu/wmt/server.cgi</OnlineResource>
    <!-- Fees or access constraints imposed. -->
    <Fees>none</Fees>
    <AccessConstraints>none</AccessConstraints>
    <!-- Name space in which values are defined -->
  </Service>
  <Capability>
    <Request>
      <Map>
        <Format>
          <JPEG/>
          <GIF/>
          <TIFF/>
          <GeoTIFF/>
        </Format>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://mesonet.agron.iastate.edu/wmt/server.cgi"/>
            <Post onlineResource="http://mesonet.agron.iastate.edu/wmt/server.cgi"/>
          </HTTP>
        </DCPType>
      </Map>
      <Capabilities>
        <Format>
          <WMS_XML/>
        </Format>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://mesonet.agron.iastate.edu/wmt/server.cgi"/>
          </HTTP>
        </DCPType>
      </Capabilities>
    </Request>
    <Exception>
      <Format>
       <WMS_XML/>
      </Format>
    </Exception>

      <Layer queryable="0">
        <Name>dmx_radar</Name>
        <Title>Des Moines Weather Radar EPSG:26975</Title>
        <Abstract>Des Moines Weather Radar Image</Abstract>
        <BoundingBox SRS="EPSG:26975" minx="1224902" miny="766405" maxx="1772741" maxy="1237197"/>
        <BoundingBox SRS="EPSG:26915" minx="1224902" miny="766405" maxx="1772741" maxy="1237197"/>
      </Layer>
      
      <Layer queryable="0">
        <Name>compradar_mw</Name>
        <Title>Iowa Composite Weather Radar EPSG:26975</Title>
        <Abstract>Iowa Composite Weather Radar Image</Abstract>
        <BoundingBox SRS="EPSG:26975" minx="1200423" miny="826311" maxx="1781223" maxy="1261911"/>
        <BoundingBox SRS="EPSG:26915" minx="1200423" miny="826311" maxx="1781223" maxy="1261911"/>
      </Layer>
    </Layer>  
  </Capability>
</WMT_MS_Capabilities>
